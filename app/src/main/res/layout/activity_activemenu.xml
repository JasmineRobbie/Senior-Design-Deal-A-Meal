<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.teamcarl.prototype.com.NeuralNet.prototype.Activemenu"
    android:background="@drawable/activemenu">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:rowCount="8"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_gravity="top"
        android:layout_alignParentTop="true">


        <TextView
            android:id="@+id/brandName"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="center"
            android:layout_marginTop="31dp"
            android:layout_marginEnd="112dp"
            android:layout_marginRight="112dp"
            android:alpha=".8"
            android:paddingTop="25dp"
            android:text="BRAND TITLE DISPLAYS HERE"
            android:textColor="@color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/activeGuideTitle"
            android:layout_width="wrap_content"
            android:layout_height="55dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="center"
            android:layout_marginTop="93dp"
            android:layout_marginEnd="79dp"
            android:layout_marginRight="79dp"
            android:layout_marginBottom="0dp"
            android:alpha=".8"
            android:text="active GUIDE title goes here"
            android:textColor="@color/white"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/activeGuidePurpose"
            android:layout_width="329dp"
            android:layout_height="119dp"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center"
            android:layout_marginEnd="27dp"
            android:layout_marginRight="27dp"
            android:layout_marginBottom="441dp"
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:text="active GUIDE purpose is displayed here!"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <ImageButton
            android:id="@+id/useactivebutton"
            android:layout_width="237dp"
            android:layout_height="58dp"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginEnd="86dp"
            android:layout_marginRight="86dp"
            android:layout_marginBottom="295dp"
            android:alpha="0"
            android:background="@drawable/useactiveguide" />

        <ImageButton
            android:id="@+id/exitbutton"
            android:layout_width="243dp"
            android:layout_height="56dp"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginEnd="83dp"
            android:layout_marginRight="83dp"
            android:layout_marginBottom="233dp"
            android:alpha="0"
            android:background="@drawable/useactiveguide" />

        <ImageButton
            android:id="@+id/newguidebutton"
            android:layout_width="243dp"
            android:layout_height="49dp"
            android:layout_row="5"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="83dp"
            android:layout_marginRight="83dp"
            android:layout_marginBottom="175dp"
            android:adjustViewBounds="false"
            android:alpha="0"
            android:background="@drawable/useactiveguide" />

        <ImageButton
            android:id="@+id/checkmemosbutton"
            android:layout_width="240dp"
            android:layout_height="62dp"
            android:layout_row="7"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="85dp"
            android:layout_marginRight="85dp"
            android:layout_marginBottom="52dp"
            android:alpha="0"
            android:background="@drawable/checkmemos" />

        <ImageButton
            android:id="@+id/weeklyGoalsButton"
            android:layout_width="240dp"
            android:layout_height="62dp"
            android:layout_row="7"
            android:layout_column="0"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="90dp"
            android:layout_marginRight="90dp"
            android:layout_marginBottom="116dp"
            android:alpha="0"
            android:background="@drawable/checkmemos" />

    </RelativeLayout>
</RelativeLayout>