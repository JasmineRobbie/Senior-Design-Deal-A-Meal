<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aim_eval_referral"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.teamcarl.prototype.com.NeuralNet.prototype.FinalEvaluation">


    <TextView
        android:id="@+id/referralText"
        android:layout_width="324dp"
        android:layout_height="164dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="38dp"
        android:backgroundTint="@color/abc_background_cache_hint_selector_material_light"
        android:backgroundTintMode="add"
        android:paddingLeft="5dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Referal Text" />

    <TextView
        android:id="@+id/brandName"
        android:layout_width="377dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="663dp"
        android:singleLine="false"
        android:text="Group code/name"
        android:textAlignment="textStart"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="163dp"
        android:layout_height="251dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="210dp"
        android:layout_marginBottom="407dp"
        android:src="@drawable/einstein"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/sendresponse"
        android:layout_width="169dp"
        android:layout_height="52dp"
        android:layout_marginStart="195dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="597dp"
        android:alpha="0"
        android:background="@drawable/sendresponse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/urllink"
        android:layout_width="170dp"
        android:layout_height="104dp"
        android:layout_marginStart="229dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="481dp"
        android:alpha="0"
        android:background="@drawable/urllink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.416" />

    <ImageButton
        android:id="@+id/mainMenu"
        android:layout_width="159dp"
        android:layout_height="59dp"
        android:layout_marginStart="210dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="410dp"
        android:alpha="0"
        android:background="@drawable/guideselect"
        android:rotation="0"
        android:rotationX="0"
        android:text="Main Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/ActiveGuideText"
        android:layout_width="327dp"
        android:layout_height="135dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="373dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="228dp"
        android:text="User and Active Guide Info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
